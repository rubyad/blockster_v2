                </div>
                </div>
                <!-- End Left: Card Rows -->

                <!-- Right: Desktop Sidebar (Position 6) -->
                <div class="md:w-1/3 w-full md:flex hidden flex-col">
                  <%= if large_interview_post = Enum.at(@posts, 5) do %>
                    <div class="inner-wrapper rounded-[18px] flex flex-col flex-1 relative overflow-hidden">
                      <%= if assigns[:current_user] && @current_user.is_admin do %>
                        <button
                          phx-click="open_post_selector"
                          phx-value-section="conversations"
                          phx-value-position="6"
                          class="absolute top-4 right-4 z-10 bg-white rounded-full p-2 shadow-md hover:bg-gray-100 transition-colors cursor-pointer"
                        >
                          <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 text-[#515B70]" viewBox="0 0 20 20" fill="currentColor">
                            <path fill-rule="evenodd" d="M11.49 3.17c-.38-1.56-2.6-1.56-2.98 0a1.532 1.532 0 01-2.286.948c-1.372-.836-2.942.734-2.106 2.106.54.886.061 2.042-.947 2.287-1.561.379-1.561 2.6 0 2.978a1.532 1.532 0 01.947 2.287c-.836 1.372.734 2.942 2.106 2.106a1.532 1.532 0 012.287.947c.379 1.561 2.6 1.561 2.978 0a1.533 1.533 0 012.287-.947c1.372.836 2.942-.734 2.106-2.106a1.533 1.533 0 01.947-2.287c1.561-.379 1.561-2.6 0-2.978a1.532 1.532 0 01-.947-2.287c.836-1.372-.734-2.942-2.106-2.106a1.532 1.532 0 01-2.287-.947zM10 13a3 3 0 100-6 3 3 0 000 6z" clip-rule="evenodd" />
                          </svg>
                        </button>
                      <% end %>
                      <.link navigate={~p"/#{large_interview_post.slug}"} class="flex-1 flex flex-col relative">
                        <!-- Full cover image -->
                        <img src={large_interview_post.featured_image} alt={large_interview_post.title} class="absolute inset-0 w-full h-full object-cover" />
                        <!-- Gradient overlay for text readability -->
                        <div class="absolute inset-0 bg-gradient-to-t from-black/70 via-black/20 to-transparent"></div>
                        <!-- Content at bottom -->
                        <div class="mt-auto p-6 z-10 relative">
                          <%= if large_interview_post.category do %>
                            <span class="px-3 py-1 bg-white border border-[#E7E8F1] text-[#141414] rounded-full text-xs font-haas_medium_65">
                              <%= large_interview_post.category.name %>
                            </span>
                          <% end %>
                          <h4 class="text-[32px] text-white font-haas_medium_65 leading-[1.2] mt-3">
                            <%= large_interview_post.title %>
                          </h4>
                          <p class="text-sm font-haas_roman_55 text-white/80 mt-2"><%= if large_interview_post.published_at, do: Calendar.strftime(large_interview_post.published_at, "%B %d, %Y"), else: "Draft" %></p>
                          <div class="p-[2px] bg-gradient-to-b from-[#8AE388] via-[#BAF55F] to-[#BAF55F] rounded-[100px] inline-block mt-3">
                            <button class="flex items-center gap-1.5 bg-white rounded-[100px] px-1 py-1 min-w-[73px]">
                              <.lightning_icon id="bux_icon_15" size="20" />
                              <span class="text-xs font-haas_medium_65"><%= Number.Delimit.number_to_delimited(Map.get(large_interview_post, :bux_balance, 0), precision: 1) %></span>
                            </button>
                          </div>
                        </div>
                      </.link>
                    </div>
                  <% end %>
                </div>
      </div>
    </div>
  </div>
</section>
