                    <h3 class="text-sm font-haas_medium_65 text-[#343434] mb-2">Send ROGUE <span class="font-haas_bold_75">to</span> Your Blockster Wallet</h3>
                    <p class="text-xs text-gray-600 mb-3">Transfer ROGUE on Rogue Chain from your <%= String.capitalize(@connected_wallet.provider) %> wallet to your Blockster smart wallet</p>

                    <%= if @wallet_balances do %>
                      <%# Get ROGUE balance on Rogue Chain only (chain_id: 560013) %>
                      <% rogue_chains = get_in(@wallet_balances, ["ROGUE", :chains]) || [] %>
                      <% rogue_balance = Enum.find_value(rogue_chains, 0.0, fn chain -> if chain.chain_id == 560013, do: chain.balance end) %>

                      <div class="mb-3">
                        <p class="text-xs text-gray-500 mb-1">Available on Rogue Chain:</p>
                        <p class="text-sm font-haas_medium_65 text-[#343434]"><%= Number.Delimit.number_to_delimited(rogue_balance, precision: 2) %> ROGUE</p>
                      </div>

                      <form phx-submit="initiate_transfer_to_blockster" class="space-y-3">
                        <div>
                          <input
                            type="number"
                            name="amount"
                            step="0.01"
                            min="0.01"
                            max={rogue_balance}
                            placeholder="Amount (ROGUE)"
                            class="w-full px-3 py-2 border border-gray-300 rounded-lg text-sm focus:outline-none focus:ring-2 focus:ring-blue-500"
                            required
                          />
                        </div>

                        <button
                          type="submit"
                          class="w-full px-4 py-2 bg-blue-600 hover:bg-blue-700 text-white rounded-full text-sm font-haas_medium_65 cursor-pointer transition-colors disabled:opacity-50 disabled:cursor-not-allowed"
                          disabled={rogue_balance <= 0}
                        >
                          <%= if rogue_balance > 0, do: "Send to Blockster", else: "No ROGUE on Rogue Chain" %>
                        </button>
                      </form>
                    <% else %>
                      <p class="text-xs text-gray-500 italic">Loading balances...</p>
                    <% end %>
                  </div>

