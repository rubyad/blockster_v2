<div class="profile-main bg-bg-light pt-28">
  <section class="lg:mb-40">
    <div class="container mx-auto">
      <div class="w-full max-w-4xl mx-auto">
        <!-- Profile Header Card -->
        <div class="bg-white rounded-lg shadow-lg overflow-hidden mb-6">
          <!-- Cover Image/Gradient -->
          <div class="h-32 bg-gradient-to-r from-[#8AE388] to-[#BAF55F]"></div>

          <!-- Profile Info -->
          <div class="px-6 pb-6">
            <div class="flex flex-col sm:flex-row items-start sm:items-end -mt-16 mb-4">
              <!-- Avatar -->
              <div class="relative mb-4 sm:mb-0">
                <div class="w-32 h-32 rounded-full border-4 border-white bg-[#AFB5FF] overflow-hidden shadow-lg">
                  <%= if @member.avatar_url do %>
                    <img src={@member.avatar_url} alt="Avatar" class="w-full h-full object-cover" />
                  <% else %>
                    <img src="/images/avatar.png" alt="Avatar" class="w-full h-full object-cover" />
                  <% end %>
                </div>
              </div>

              <!-- Name and Username -->
              <div class="sm:ml-6 flex-1">
                <h1 class="text-3xl font-haas_bold_75 text-gray-900">
                  <%= @member.username || "User" %>
                </h1>
                <%= if @member.smart_wallet_address do %>
                  <div class="flex items-center gap-2 mt-1">
                    <a
                      href={"https://roguescan.io/address/#{@member.smart_wallet_address}?tab=token_transfers"}
                      target="_blank"
                      rel="noopener noreferrer"
                      class="text-sm text-gray-500 hover:text-blue-500 cursor-pointer"
                    >
                      <%= @member.smart_wallet_address %>
                    </a>
                    <button
                      type="button"
                      id={"copy-wallet-#{@member.id}"}
                      phx-hook="CopyToClipboard"
                      data-copy-text={@member.smart_wallet_address}
                      class="text-gray-400 hover:text-gray-600 cursor-pointer"
                      title="Copy address"
                    >
                      <svg xmlns="http://www.w3.org/2000/svg" class="h-4 w-4" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 16H6a2 2 0 01-2-2V6a2 2 0 012-2h8a2 2 0 012 2v2m-6 12h8a2 2 0 002-2v-8a2 2 0 00-2-2h-8a2 2 0 00-2 2v8a2 2 0 002 2z" />
                      </svg>
                    </button>
                  </div>
                <% else %>
                  <p class="text-sm text-gray-500 mt-1">
                    Member since <%= Calendar.strftime(@member.inserted_at, "%B %Y") %>
                  </p>
                <% end %>
              </div>
            </div>
          </div>
        </div>

        <!-- Phone Verification Banner (shown only to own profile, unverified) -->
        <%= if @current_user && @current_user.id == @member.id && (!@member.phone_verification || !@member.phone_verification.verified) do %>
          <div class="relative bg-gradient-to-r from-gray-50 to-gray-100 rounded-xl p-6 mb-6 overflow-hidden">
            <!-- Animated border -->
            <div class="absolute inset-0 rounded-xl" style="padding: 2px;">
              <div class="absolute inset-0 rounded-xl bg-gradient-to-r from-black via-gray-400 to-black animate-spin-slow" style="animation: spin 3s linear infinite;"></div>
              <div class="absolute inset-[2px] rounded-xl bg-gradient-to-r from-gray-50 to-gray-100"></div>
            </div>

            <!-- Content -->
            <div class="relative flex items-center justify-between">
              <div class="flex items-center gap-4">
                <div class="flex-shrink-0 w-12 h-12 bg-black rounded-full flex items-center justify-center">
                  <svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 5a2 2 0 012-2h3.28a1 1 0 01.948.684l1.498 4.493a1 1 0 01-.502 1.21l-2.257 1.13a11.042 11.042 0 005.516 5.516l1.13-2.257a1 1 0 011.21-.502l4.493 1.498a1 1 0 01.684.949V19a2 2 0 01-2 2h-1C9.716 21 3 14.284 3 6V5z"/>
                  </svg>
                </div>
                <div>
                  <h3 class="text-lg font-haas_medium_65 text-gray-900">
                    Verify your phone number to double your BUX earnings
                  </h3>
                </div>
              </div>
              <button
                phx-click="open_phone_verification"
                class="bg-black text-white px-6 py-3 rounded-lg font-haas_medium_65 hover:bg-gray-800 transition cursor-pointer shadow-md hover:shadow-lg"
              >
                Verify Now ‚Üí
              </button>
            </div>
          </div>

          <style>
            @keyframes spin {
              from { transform: rotate(0deg); }
              to { transform: rotate(360deg); }
            }
          </style>
        <% end %>

        <!-- Verified Badge (shown for verified users in profile info section) -->
        <%= if @member.phone_verification && @member.phone_verification.verified do %>
          <div class="bg-white rounded-lg shadow-lg p-4 mb-6">
            <div class="flex items-center justify-between">
              <div class="inline-flex items-center px-3 py-1 rounded-full bg-green-100 border border-green-300 text-sm">
                <svg class="w-4 h-4 text-green-600 mr-1" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M6.267 3.455a3.066 3.066 0 001.745-.723 3.066 3.066 0 013.976 0 3.066 3.066 0 001.745.723 3.066 3.066 0 012.812 2.812c.051.643.304 1.254.723 1.745a3.066 3.066 0 010 3.976 3.066 3.066 0 00-.723 1.745 3.066 3.066 0 01-2.812 2.812 3.066 3.066 0 00-1.745.723 3.066 3.066 0 01-3.976 0 3.066 3.066 0 00-1.745-.723 3.066 3.066 0 01-2.812-2.812 3.066 3.066 0 00-.723-1.745 3.066 3.066 0 010-3.976 3.066 3.066 0 00.723-1.745 3.066 3.066 0 012.812-2.812zm7.44 5.252a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"/>
                </svg>
                <span class="text-green-800 font-haas_medium_65">Phone Verified</span>
                <span class="ml-2 text-green-600">‚Ä¢ <%= @member.phone_verification.geo_multiplier %>x earnings</span>
              </div>
              <div class="text-sm text-gray-600">
                üåç <%= @member.phone_verification.country_code %>
              </div>
            </div>
          </div>
        <% end %>

        <!-- Anonymous Claim Success Message (for new users only) -->
        <%= if @is_new_user && @total_claimed > 0 do %>
          <div class="bg-gradient-to-r from-green-50 to-emerald-50 border-2 border-green-400 rounded-lg shadow-lg p-6 mb-6" phx-hook="ClaimCleanup" id="claim-success-banner">
            <div class="flex items-start gap-4">
              <div class="flex-shrink-0">
                <div class="w-12 h-12 rounded-full bg-green-500 flex items-center justify-center">
                  <svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"/>
                  </svg>
                </div>
              </div>
              <div class="flex-1">
                <h3 class="text-xl font-haas_bold_75 text-green-900 mb-2">
                  üéâ Welcome to Blockster!
                </h3>
                <p class="text-green-800 font-haas_medium_65 mb-3">
                  You successfully claimed <span class="font-haas_bold_75 text-2xl"><%= Float.round(@total_claimed, 2) %> BUX</span> from <%= @claim_type_text || "engaging with content" %>!
                </p>
                <div class="text-sm text-green-700">
                  Check your activity feed below to see the transaction details.
                </div>
              </div>
            </div>
          </div>
        <% end %>

        <!-- How It Works Section (for new users only) -->
        <%= if @is_new_user do %>
          <div class="bg-white rounded-lg shadow-lg p-6 mb-6">
            <h2 class="text-2xl font-haas_bold_75 text-gray-900 mb-4">How It Works</h2>

            <div class="space-y-4">
              <!-- Step 1: Connect X -->
              <div class="flex items-start gap-4">
                <div class="flex-shrink-0 w-10 h-10 rounded-full bg-gradient-to-r from-[#8AE388] to-[#BAF55F] flex items-center justify-center text-black font-haas_bold_75">
                  1
                </div>
                <div class="flex-1">
                  <h3 class="font-haas_bold_75 text-gray-900 mb-1">Connect Your X Account</h3>
                  <p class="text-gray-600 text-sm">
                    Link your X (Twitter) account to earn bonus BUX when you share posts and build your multiplier.
                  </p>
                </div>
              </div>

              <!-- Step 2: Earn BUX -->
              <div class="flex items-start gap-4">
                <div class="flex-shrink-0 w-10 h-10 rounded-full bg-gradient-to-r from-[#8AE388] to-[#BAF55F] flex items-center justify-center text-black font-haas_bold_75">
                  2
                </div>
                <div class="flex-1">
                  <h3 class="font-haas_bold_75 text-gray-900 mb-1">Earn More BUX</h3>
                  <p class="text-gray-600 text-sm">
                    Read articles, watch videos, and share content to earn BUX tokens. Your multiplier increases your rewards!
                  </p>
                </div>
              </div>

              <!-- Step 3: Redeem -->
              <div class="flex items-start gap-4">
                <div class="flex-shrink-0 w-10 h-10 rounded-full bg-gradient-to-r from-[#8AE388] to-[#BAF55F] flex items-center justify-center text-black font-haas_bold_75">
                  3
                </div>
                <div class="flex-1">
                  <h3 class="font-haas_bold_75 text-gray-900 mb-1">Redeem Your BUX</h3>
                  <p class="text-gray-600 text-sm mb-2">Use your BUX for:</p>
                  <ul class="list-disc list-inside text-gray-600 text-sm space-y-1">
                    <li><strong>Shopping discounts</strong> - Get 10% off per BUX in the shop</li>
                    <li><strong>Airdrop entries</strong> - Enter exclusive airdrops</li>
                    <li><strong>BUX Booster</strong> - Play the coin flip game for ROGUE rewards</li>
                  </ul>
                </div>
              </div>
            </div>

            <!-- CTA Buttons -->
            <div class="mt-6 flex flex-col sm:flex-row gap-3">
              <.link
                navigate={~p"/"}
                class="flex-1 text-center bg-gradient-to-r from-[#8AE388] to-[#BAF55F] text-black font-haas_medium_65 py-3 px-6 rounded-lg hover:shadow-lg transition-all cursor-pointer"
              >
                Start Reading & Earning
              </.link>
              <.link
                navigate={~p"/play"}
                class="flex-1 text-center bg-white border-2 border-gray-300 text-gray-700 font-haas_medium_65 py-3 px-6 rounded-lg hover:border-gray-400 transition-all cursor-pointer"
              >
                Try BUX Booster
              </.link>
            </div>
          </div>
        <% end %>

        <!-- Stats Grid -->
        <div class="grid grid-cols-1 md:grid-cols-3 gap-6 mb-6">
          <!-- BUX Balance Card -->
          <div class="bg-white rounded-lg shadow p-6">
            <div class="flex items-center justify-between mb-2">
              <h3 class="text-sm font-haas_medium_65 text-gray-500 uppercase">BUX Balance</h3>
              <.lightning_icon id="member_bux" size="24" />
            </div>
            <p class="text-3xl font-haas_bold_75 text-gray-900 tracking-wide">
              <%= Number.Currency.number_to_currency(Map.get(@token_balances, "BUX", 0), unit: "", precision: 2) %> BUX
            </p>
          </div>

          <!-- Level Card with Dropdown -->
          <div class="relative" phx-click-away="close_multiplier_dropdown">
            <div
              class="bg-white rounded-lg shadow p-6 cursor-pointer hover:shadow-md transition-shadow"
              phx-click="toggle_multiplier_dropdown"
            >
              <div class="flex items-center justify-between mb-2">
                <h3 class="text-sm font-haas_medium_65 text-gray-500 uppercase">Level</h3>
                <div class="flex items-center gap-2">
                  <svg xmlns="http://www.w3.org/2000/svg" class="h-6 w-6 text-[#8AE388]" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 10V3L4 14h7v7l9-11h-7z" />
                  </svg>
                  <svg
                    xmlns="http://www.w3.org/2000/svg"
                    class={"h-4 w-4 text-gray-400 transition-transform #{if @show_multiplier_dropdown, do: "rotate-180", else: ""}"}
                    fill="none"
                    viewBox="0 0 24 24"
                    stroke="currentColor"
                  >
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 9l-7 7-7-7" />
                  </svg>
                </div>
              </div>
              <p class="text-3xl font-haas_bold_75 text-gray-900">
                <%= @overall_multiplier %>
              </p>
              <p class="text-xs text-gray-500 mt-1">Click to view multiplier breakdown</p>
            </div>

            <!-- Multiplier Breakdown Dropdown -->
            <%= if @show_multiplier_dropdown do %>
              <div class="absolute top-full left-0 right-0 mt-2 bg-white rounded-lg shadow-lg border border-gray-200 z-50 overflow-hidden">
                <div class="p-3 bg-gradient-to-r from-[#8AE388] to-[#BAF55F]">
                  <h4 class="text-sm font-haas_bold_75 text-black">Multiplier Breakdown</h4>
                </div>
                <div class="divide-y divide-gray-100">
                  <!-- X (Twitter) Multiplier -->
                  <div class="flex items-center justify-between px-4 py-3">
                    <div class="flex items-center gap-2">
                      <svg class="w-5 h-5" viewBox="0 0 24 24" fill="currentColor"><path d="M18.244 2.25h3.308l-7.227 8.26 8.502 11.24H16.17l-5.214-6.817L4.99 21.75H1.68l7.73-8.835L1.254 2.25H8.08l4.713 6.231zm-1.161 17.52h1.833L7.084 4.126H5.117z"/></svg>
                      <span class="text-sm font-haas_medium_65 text-gray-900">X Account</span>
                    </div>
                    <span class={"text-sm font-haas_bold_75 #{if @multiplier_details.x_multiplier > 1, do: "text-green-600", else: "text-gray-400"}"}><%= @multiplier_details.x_multiplier %>x</span>
                  </div>
                  <!-- LinkedIn Multiplier -->
                  <div class="flex items-center justify-between px-4 py-3">
                    <div class="flex items-center gap-2">
                      <svg class="w-5 h-5 text-[#0A66C2]" viewBox="0 0 24 24" fill="currentColor"><path d="M20.447 20.452h-3.554v-5.569c0-1.328-.027-3.037-1.852-3.037-1.853 0-2.136 1.445-2.136 2.939v5.667H9.351V9h3.414v1.561h.046c.477-.9 1.637-1.85 3.37-1.85 3.601 0 4.267 2.37 4.267 5.455v6.286zM5.337 7.433c-1.144 0-2.063-.926-2.063-2.065 0-1.138.92-2.063 2.063-2.063 1.14 0 2.064.925 2.064 2.063 0 1.139-.925 2.065-2.064 2.065zm1.782 13.019H3.555V9h3.564v11.452zM22.225 0H1.771C.792 0 0 .774 0 1.729v20.542C0 23.227.792 24 1.771 24h20.451C23.2 24 24 23.227 24 22.271V1.729C24 .774 23.2 0 22.222 0h.003z"/></svg>
                      <span class="text-sm font-haas_medium_65 text-gray-900">LinkedIn</span>
                    </div>
                    <span class={"text-sm font-haas_bold_75 #{if @multiplier_details.linkedin_multiplier > 1, do: "text-green-600", else: "text-gray-400"}"}><%= @multiplier_details.linkedin_multiplier %>x</span>
                  </div>
                  <!-- ROGUE Staked Multiplier -->
                  <div class="flex items-center justify-between px-4 py-3">
                    <div class="flex items-center gap-2">
                      <img src="https://ik.imagekit.io/blockster/rogue-white-in-indigo-logo.png" alt="ROGUE" class="w-5 h-5 rounded-full" />
                      <span class="text-sm font-haas_medium_65 text-gray-900">ROGUE Staked</span>
                    </div>
                    <span class={"text-sm font-haas_bold_75 #{if @multiplier_details.rogue_multiplier > 1, do: "text-green-600", else: "text-gray-400"}"}><%= @multiplier_details.rogue_multiplier %>x</span>
                  </div>
                  <!-- Personal Multiplier -->
                  <div class="flex items-center justify-between px-4 py-3">
                    <div class="flex items-center gap-2">
                      <svg xmlns="http://www.w3.org/2000/svg" class="w-5 h-5 text-purple-500" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M16 7a4 4 0 11-8 0 4 4 0 018 0zM12 14a7 7 0 00-7 7h14a7 7 0 00-7-7z" />
                      </svg>
                      <span class="text-sm font-haas_medium_65 text-gray-900">Personal</span>
                    </div>
                    <span class={"text-sm font-haas_bold_75 #{if @multiplier_details.personal_multiplier > 1, do: "text-green-600", else: "text-gray-400"}"}><%= @multiplier_details.personal_multiplier %>x</span>
                  </div>
                  <!-- Overall Total -->
                  <div class="flex items-center justify-between px-4 py-3 bg-gray-50">
                    <div class="flex items-center gap-2">
                      <svg xmlns="http://www.w3.org/2000/svg" class="w-5 h-5 text-[#8AE388]" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 10V3L4 14h7v7l9-11h-7z" />
                      </svg>
                      <span class="text-sm font-haas_bold_75 text-gray-900">Overall Level</span>
                    </div>
                    <span class="text-sm font-haas_bold_75 text-[#8AE388]"><%= @overall_multiplier %>x</span>
                  </div>
                </div>
              </div>
            <% end %>
          </div>

          <!-- ROGUE Balance Card -->
          <div class="bg-white rounded-lg shadow p-6">
            <div class="flex items-center justify-between mb-2">
              <h3 class="text-sm font-haas_medium_65 text-gray-500 uppercase">ROGUE Balance</h3>
              <img src="https://ik.imagekit.io/blockster/rogue-white-in-indigo-logo.png" alt="ROGUE" class="h-6 w-6 rounded-full" />
            </div>
            <p class="text-3xl font-haas_bold_75 text-gray-900">
              <%= Number.Currency.number_to_currency(Map.get(@token_balances, "ROGUE", 0), unit: "", precision: 2) %>
            </p>
            <p class="text-xs text-gray-500 mt-1">
              <a href="https://app.uniswap.org/swap?outputCurrency=0x7D07645B9dDc0A7f6a18E67B9AA08A3b7cD6d49F" target="_blank" class="text-blue-500 hover:underline cursor-pointer">Buy ROGUE</a>
            </p>
          </div>
        </div>
        <div class="w-full md:bg-white rounded-2xl md:p-6 md:p-3 mt-3">
          <div class="p-2 bg-[#E7E8F1] rounded-full max-md:hidden">
            <div class="inline-flex w-full gap-1">
              <button
                phx-click="switch_tab"
                phx-value-tab="activity"
                class={"w-1/3 px-3 py-2.5 font-haas_roman_55 text-sm text-[#141414] leading-[1.4] cursor-pointer rounded-full transition-colors relative z-10 #{if @active_tab == "activity", do: "bg-white", else: ""}"}
              >
                Activity
              </button>
              <%# Temporarily hidden - Following tab %>
              <%# <button
                phx-click="switch_tab"
                phx-value-tab="following"
                class={"w-1/6 px-3 py-2.5 font-haas_roman_55 text-sm text-[#141414] leading-[1.4] cursor-pointer rounded-full transition-colors relative z-10 #{if @active_tab == "following", do: "bg-white", else: ""}"}
              >
                Following
              </button> %>
              <%# Temporarily hidden - Events tab %>
              <%# <button
                phx-click="switch_tab"
                phx-value-tab="event"
                class={"w-1/6 px-3 py-2.5 font-haas_roman_55 text-sm text-[#141414] leading-[1.4] cursor-pointer rounded-full transition-colors relative z-10 #{if @active_tab == "event", do: "bg-white", else: ""}"}
              >
                Events
              </button> %>
              <button
                phx-click="switch_tab"
                phx-value-tab="wallet"
                class={"w-1/3 px-3 py-2.5 font-haas_roman_55 text-sm text-[#141414] leading-[1.4] cursor-pointer rounded-full transition-colors relative z-10 #{if @active_tab == "wallet", do: "bg-white", else: ""}"}
              >
                External Wallet
              </button>
              <%# Temporarily hidden - Airdrop tab %>
              <%# <button
                phx-click="switch_tab"
                phx-value-tab="airdrop"
                class={"w-1/6 px-3 py-2.5 font-haas_roman_55 text-sm text-[#141414] leading-[1.4] cursor-pointer rounded-full transition-colors relative z-10 #{if @active_tab == "airdrop", do: "bg-white", else: ""}"}
              >
                Airdrop
              </button> %>
              <button
                phx-click="switch_tab"
                phx-value-tab="settings"
                class={"w-1/3 px-3 py-2.5 font-haas_roman_55 text-sm text-[#141414] leading-[1.4] cursor-pointer rounded-full transition-colors relative z-10 #{if @active_tab == "settings", do: "bg-white", else: ""}"}
              >
                Settings
              </button>
            </div>
          </div>
          <div class={"tab-wrapper relative #{if @active_tab != "following", do: "hidden", else: ""}"} id="following">
            <div class="tab-secondary flex md:hidden">
              <button class="!text-[14px] font-haas_medium_65 text-[#343434] leading-[24px] p-2 border-[#141414] border-b border-solid">
                Current Pool
              </button>
              <button class="!text-[14px] flex gap-2 items-center font-haas_medium_55 text-[#343434] leading-[24px] p-2 border-[#141414] disabled:opacity-[0.6]">
                <span>
                  Business Accounts
                </span>
              </button>
              <button class="!text-[14px] flex gap-2 items-center font-haas_medium_55 text-[#343434] leading-[24px] p-2 border-[#141414] disabled:opacity-[0.6]">
                <span>
                  Users
                </span>
              </button>
            </div>
            <div class="cards-wrapper grid sm:grid-cols-3 grid-cols-1 gap-3 mt-3 max-md:p-3 max-md:bg-white rounded-[12px]">
              <div class="card p-3 border border-[#E8EAEC] rounded-[12px]">
                <div class="flex items-center gap-2">
                  <span class="block">
                    <svg fill="none" height="32" viewbox="0 0 32 32" width="32" xmlns="http://www.w3.org/2000/svg">
