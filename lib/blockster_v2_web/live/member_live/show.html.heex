<div class="profile-main bg-wallet-bg min-h-screen pt-16 lg:pt-24">
  <section class="lg:mb-40">
    <div class="container mx-auto">
      <div class="w-full max-w-4xl mx-auto">
        <!-- Profile Header Card -->
        <div class="relative overflow-hidden rounded-2xl mb-6 bg-wallet-card border border-gray-200">
          <!-- Geometric accent pattern -->
          <div class="absolute top-0 right-0 w-64 h-64 bg-gray-50 rounded-full blur-3xl -translate-y-1/2 translate-x-1/2"></div>
          <div class="absolute bottom-0 left-0 w-48 h-48 bg-gray-200/50 rounded-full blur-3xl translate-y-1/2 -translate-x-1/4"></div>

          <!-- Profile Info -->
          <div class="relative px-6 py-8 sm:px-8">
            <div class="flex flex-col sm:flex-row items-start sm:items-center gap-5">
              <!-- Blockster Logo -->
              <div class="relative flex-shrink-0">
                <img src="https://ik.imagekit.io/blockster/blockster-icon.png" alt="Blockster" class="w-16 h-16 rounded-2xl" />
              </div>

              <!-- Name and Username -->
              <div class="flex-1 min-w-0">
                <h1 class="text-2xl sm:text-3xl font-haas_bold_75 text-gray-900 tracking-tight">
                  My Blockster Wallet
                </h1>
                <%= if @member.smart_wallet_address do %>
                  <div class="flex items-center gap-2 mt-2">
                    <a
                      href={"https://roguescan.io/address/#{@member.smart_wallet_address}?tab=token_transfers"}
                      target="_blank"
                      rel="noopener noreferrer"
                      class="inline-flex items-center gap-1.5 px-3 py-1.5 bg-gray-100 border border-gray-200 rounded-lg text-xs font-mono text-gray-400 hover:text-gray-900 hover:border-gray-200 transition-all cursor-pointer truncate max-w-[280px] sm:max-w-none"
                    >
                      <%= @member.smart_wallet_address %>
                    </a>
                    <button
                      type="button"
                      id={"copy-wallet-#{@member.id}"}
                      phx-hook="CopyToClipboard"
                      data-copy-text={@member.smart_wallet_address}
                      class="p-1.5 rounded-lg bg-gray-100 border border-gray-200 text-gray-500 hover:text-gray-900 hover:border-gray-200 transition-all cursor-pointer flex-shrink-0"
                      title="Copy address"
                    >
                      <svg xmlns="http://www.w3.org/2000/svg" class="h-3.5 w-3.5" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 16H6a2 2 0 01-2-2V6a2 2 0 012-2h8a2 2 0 012 2v2m-6 12h8a2 2 0 002-2v-8a2 2 0 00-2-2h-8a2 2 0 00-2 2v8a2 2 0 002 2z" />
                      </svg>
                    </button>
                  </div>
                <% else %>
                  <p class="text-sm text-gray-500 mt-1">
                    Member since <%= Calendar.strftime(@member.inserted_at, "%B %Y") %>
                  </p>
                <% end %>
              </div>
            </div>
          </div>
        </div>

        <!-- Phone Verification Banner (shown only to own profile, unverified) -->
        <%= if @current_user && @current_user.id == @member.id && (!@member.phone_verification || !@member.phone_verification.verified) do %>
          <div class="relative bg-wallet-card border border-gray-200 rounded-2xl p-5 sm:p-6 mb-6 overflow-hidden">
            <div class="absolute top-0 left-0 w-full h-[2px] bg-gradient-to-r from-transparent via-gray-300 to-transparent"></div>
            <div class="flex flex-col sm:flex-row sm:items-center sm:justify-between gap-4">
              <div class="flex items-center gap-4">
                <div class="flex-shrink-0 w-10 h-10 sm:w-11 sm:h-11 bg-gray-900 rounded-xl flex items-center justify-center">
                  <svg class="w-5 h-5 sm:w-5 sm:h-5 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 5a2 2 0 012-2h3.28a1 1 0 01.948.684l1.498 4.493a1 1 0 01-.502 1.21l-2.257 1.13a11.042 11.042 0 005.516 5.516l1.13-2.257a1 1 0 011.21-.502l4.493 1.498a1 1 0 01.684.949V19a2 2 0 01-2 2h-1C9.716 21 3 14.284 3 6V5z"/>
                  </svg>
                </div>
                <div>
                  <h3 class="text-sm sm:text-base font-haas_medium_65 text-gray-900">
                    Verify your phone to boost BUX earnings
                  </h3>
                  <p class="text-xs text-gray-500 mt-0.5">Up to 2x multiplier on all rewards</p>
                </div>
              </div>
              <button
                phx-click="open_phone_verification"
                class="bg-gray-900 text-white px-6 py-2.5 rounded-xl font-haas_medium_65 text-sm hover:bg-gray-800 transition cursor-pointer whitespace-nowrap flex-shrink-0 w-full sm:w-auto text-center"
              >
                Verify Now
              </button>
            </div>
          </div>
        <% end %>

        <!-- Verified Badge (shown for verified users in profile info section) -->
        <%= if @member.phone_verification && @member.phone_verification.verified do %>
          <div class="bg-wallet-card border border-gray-200 rounded-2xl px-5 py-3.5 mb-6">
            <div class="flex items-center justify-between">
              <div class="inline-flex items-center gap-2 px-3 py-1.5 rounded-xl bg-gray-100 border border-gray-200 text-sm">
                <svg class="w-4 h-4 text-gray-900" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M6.267 3.455a3.066 3.066 0 001.745-.723 3.066 3.066 0 013.976 0 3.066 3.066 0 001.745.723 3.066 3.066 0 012.812 2.812c.051.643.304 1.254.723 1.745a3.066 3.066 0 010 3.976 3.066 3.066 0 00-.723 1.745 3.066 3.066 0 01-2.812 2.812 3.066 3.066 0 00-1.745.723 3.066 3.066 0 01-3.976 0 3.066 3.066 0 00-1.745-.723 3.066 3.066 0 01-2.812-2.812 3.066 3.066 0 00-.723-1.745 3.066 3.066 0 010-3.976 3.066 3.066 0 00.723-1.745 3.066 3.066 0 012.812-2.812zm7.44 5.252a1 1 0 00-1.414-1.414L9 10.586 7.707 9.293a1 1 0 00-1.414 1.414l2 2a1 1 0 001.414 0l4-4z" clip-rule="evenodd"/>
                </svg>
                <span class="text-gray-900 font-haas_medium_65 text-xs">Phone Verified</span>
                <span class="text-gray-900/70 text-xs"><%= @member.phone_verification.geo_multiplier %>x</span>
              </div>
              <div class="text-xs text-gray-500">
                <%= @member.phone_verification.country_code %>
              </div>
            </div>
          </div>
        <% end %>

        <!-- Anonymous Claim Success Message (for new users only) -->
        <%= if @is_new_user && @total_claimed > 0 do %>
          <div class="bg-wallet-card border border-gray-200 rounded-2xl p-5 sm:p-6 mb-6 relative overflow-hidden" phx-hook="ClaimCleanup" id="claim-success-banner">
            <div class="absolute top-0 left-0 w-full h-[2px] bg-gradient-to-r from-transparent via-gray-300 to-transparent"></div>
            <div class="flex items-start gap-4">
              <div class="flex-shrink-0">
                <div class="w-11 h-11 rounded-xl bg-gray-900 flex items-center justify-center">
                  <svg class="w-5 h-5 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2.5" d="M5 13l4 4L19 7"/>
                  </svg>
                </div>
              </div>
              <div class="flex-1">
                <h3 class="text-lg font-haas_bold_75 text-gray-900 mb-1">
                  Welcome to Blockster!
                </h3>
                <p class="text-gray-600 font-haas_medium_65 mb-2 text-sm">
                  You claimed <span class="font-haas_bold_75 text-xl text-gray-900"><%= Float.round(@total_claimed, 2) %> BUX</span> from <%= @claim_type_text || "engaging with content" %>
                </p>
                <div class="text-xs text-gray-500">
                  Check your activity feed below for details.
                </div>
              </div>
            </div>
          </div>
        <% end %>

        <!-- How It Works Section (for new users only) -->
        <%= if @is_new_user do %>
          <div class="bg-wallet-card border border-gray-200 rounded-2xl p-6 mb-6">
            <h2 class="text-xl font-haas_bold_75 text-gray-900 mb-5">How It Works</h2>

            <div class="space-y-4">
              <!-- Step 1: Connect X -->
              <div class="flex items-start gap-4">
                <div class="flex-shrink-0 w-9 h-9 rounded-xl bg-gray-900 flex items-center justify-center text-white text-sm font-haas_bold_75">
                  1
                </div>
                <div class="flex-1">
                  <h3 class="font-haas_bold_75 text-gray-900 text-sm mb-0.5">Connect Your X Account</h3>
                  <p class="text-gray-500 text-xs leading-relaxed">
                    Link your X (Twitter) account to earn bonus BUX when you share posts and build your multiplier.
                  </p>
                </div>
              </div>

              <!-- Step 2: Earn BUX -->
              <div class="flex items-start gap-4">
                <div class="flex-shrink-0 w-9 h-9 rounded-xl bg-gray-900 flex items-center justify-center text-white text-sm font-haas_bold_75">
                  2
                </div>
                <div class="flex-1">
                  <h3 class="font-haas_bold_75 text-gray-900 text-sm mb-0.5">Earn More BUX</h3>
                  <p class="text-gray-500 text-xs leading-relaxed">
                    Read articles, watch videos, and share content to earn BUX tokens. Your multiplier increases your rewards!
                  </p>
                </div>
              </div>

              <!-- Step 3: Redeem -->
              <div class="flex items-start gap-4">
                <div class="flex-shrink-0 w-9 h-9 rounded-xl bg-gray-900 flex items-center justify-center text-white text-sm font-haas_bold_75">
                  3
                </div>
                <div class="flex-1">
                  <h3 class="font-haas_bold_75 text-gray-900 text-sm mb-0.5">Redeem Your BUX</h3>
                  <p class="text-gray-500 text-xs leading-relaxed mb-1.5">Use your BUX for:</p>
                  <ul class="text-gray-500 text-xs space-y-1">
                    <li class="flex items-center gap-1.5"><span class="w-1 h-1 rounded-full bg-gray-400 flex-shrink-0"></span><strong class="text-gray-700">Shopping discounts</strong> — up to 100% off</li>
                    <li class="flex items-center gap-1.5"><span class="w-1 h-1 rounded-full bg-gray-400 flex-shrink-0"></span><strong class="text-gray-700">Airdrop entries</strong> — exclusive airdrops</li>
                    <li class="flex items-center gap-1.5"><span class="w-1 h-1 rounded-full bg-gray-400 flex-shrink-0"></span><strong class="text-gray-700">BUX Booster</strong> — win BUX and ROGUE</li>
                  </ul>
                </div>
              </div>
            </div>

            <!-- CTA Buttons -->
            <div class="mt-6 flex flex-col sm:flex-row gap-3">
              <.link
                navigate={~p"/"}
                class="flex-1 text-center bg-gray-900 text-white font-haas_medium_65 py-3 px-6 rounded-xl text-sm hover:bg-gray-800 transition-all cursor-pointer"
              >
                Start Reading & Earning
              </.link>
              <.link
                navigate={~p"/play"}
                class="flex-1 text-center bg-gray-100 border border-gray-200 text-gray-600 font-haas_medium_65 py-3 px-6 rounded-xl text-sm hover:bg-gray-200 transition-all cursor-pointer"
              >
                Try BUX Booster
              </.link>
            </div>
          </div>
        <% end %>

        <!-- Stats Grid -->
        <div class="grid grid-cols-1 md:grid-cols-3 gap-4 mb-6">
          <!-- BUX Balance Card -->
          <div class="bg-wallet-card border border-gray-200 rounded-2xl p-5">
            <div class="flex items-center justify-between mb-3">
              <h3 class="text-[10px] font-haas_medium_65 text-gray-500 uppercase tracking-widest">BUX Balance</h3>
              <.lightning_icon id="member_bux" size="24" />
            </div>
            <p class="text-2xl sm:text-3xl font-haas_bold_75 text-gray-900 tracking-tight">
              <%= Number.Currency.number_to_currency(Map.get(@token_balances, "BUX", 0), unit: "", precision: 2) %>
            </p>
            <p class="text-[10px] text-gray-600 mt-1 font-haas_medium_65">BUX</p>
          </div>

          <!-- BUX Multiplier Card with Dropdown (V2 Unified System) -->
          <div class="relative" phx-click-away="close_multiplier_dropdown">
            <div
              class="bg-wallet-card border border-gray-200 rounded-2xl p-5 cursor-pointer hover:border-gray-300 transition-colors"
              phx-click="toggle_multiplier_dropdown"
            >
              <div class="flex items-center justify-between mb-3">
                <h3 class="text-[10px] font-haas_medium_65 text-gray-500 uppercase tracking-widest">BUX Multiplier</h3>
                <div class="flex items-center gap-2">
                  <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 text-gray-900" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 10V3L4 14h7v7l9-11h-7z" />
                  </svg>
                  <svg
                    xmlns="http://www.w3.org/2000/svg"
                    class={"h-4 w-4 text-gray-600 transition-transform #{if @show_multiplier_dropdown, do: "rotate-180", else: ""}"}
                    fill="none"
                    viewBox="0 0 24 24"
                    stroke="currentColor"
                  >
                    <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M19 9l-7 7-7-7" />
                  </svg>
                </div>
              </div>
              <p class="text-2xl sm:text-3xl font-haas_bold_75 text-gray-900 tracking-tight">
                <%= :erlang.float_to_binary(@overall_multiplier / 1, decimals: 1) %>x
              </p>
              <p class="text-[10px] text-gray-600 mt-1 font-haas_medium_65">View breakdown</p>
            </div>

            <!-- V2 Multiplier Breakdown Dropdown -->
            <%= if @show_multiplier_dropdown do %>
              <div class="absolute top-full left-0 right-0 mt-2 bg-wallet-card-alt rounded-2xl border border-gray-200 z-50 overflow-hidden shadow-xl">
                <div class="p-3 bg-gray-900">
                  <h4 class="text-sm font-haas_bold_75 text-white">Your BUX Multipliers</h4>
                  <p class="text-xs text-white/60">X × Phone × ROGUE × Wallet</p>
                </div>
                <div class="divide-y divide-gray-100">
                  <!-- X Account Multiplier -->
                  <div class="flex items-center justify-between px-4 py-3">
                    <div class="flex items-center gap-2 shrink-0">
                      <div class="w-7 h-7 rounded-lg bg-gray-100 flex items-center justify-center">
                        <svg class="w-3.5 h-3.5 text-gray-400" viewBox="0 0 24 24" fill="currentColor"><path d="M18.244 2.25h3.308l-7.227 8.26 8.502 11.24H16.17l-5.214-6.817L4.99 21.75H1.68l7.73-8.835L1.254 2.25H8.08l4.713 6.231zm-1.161 17.52h1.833L7.084 4.126H5.117z"/></svg>
                      </div>
                      <span class="text-sm font-haas_medium_65 text-gray-600">X</span>
                    </div>
                    <div class="flex items-center gap-2">
                      <%= if @x_multiplier <= 1 do %>
                        <a href={~p"/auth/x?redirect=/member/#{@member.slug || @member.smart_wallet_address}"} class="text-xs bg-gray-900 text-white w-16 py-1 rounded-lg hover:bg-gray-800 cursor-pointer font-haas_medium_65 text-center">Connect</a>
                      <% end %>
                      <div class="text-right w-14">
                        <span class={"text-sm font-haas_bold_75 #{if @x_multiplier > 1, do: "text-gray-900", else: "text-gray-600"}"}>
                          <%= :erlang.float_to_binary(@x_multiplier / 1, decimals: 1) %>x
                        </span>
                        <span class="text-xs text-gray-600 block">/ 10x</span>
                      </div>
                    </div>
                  </div>
                  <!-- Phone Verification Multiplier -->
                  <div class="flex items-center justify-between px-4 py-3">
                    <div class="flex items-center gap-2 shrink-0">
                      <div class="w-7 h-7 rounded-lg bg-gray-100 flex items-center justify-center">
                        <svg class="w-3.5 h-3.5 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 5a2 2 0 012-2h3.28a1 1 0 01.948.684l1.498 4.493a1 1 0 01-.502 1.21l-2.257 1.13a11.042 11.042 0 005.516 5.516l1.13-2.257a1 1 0 011.21-.502l4.493 1.498a1 1 0 01.684.949V19a2 2 0 01-2 2h-1C9.716 21 3 14.284 3 6V5z"/>
                        </svg>
                      </div>
                      <span class="text-sm font-haas_medium_65 text-gray-600">Phone</span>
                    </div>
                    <div class="flex items-center gap-2">
                      <%= if @phone_multiplier < 1.0 do %>
                        <button phx-click="open_phone_verification" class="text-xs bg-gray-900 text-white w-16 py-1 rounded-lg hover:bg-gray-800 cursor-pointer font-haas_medium_65 text-center">Verify</button>
                      <% end %>
                      <div class="text-right w-14">
                        <span class={"text-sm font-haas_bold_75 #{if @phone_multiplier >= 1.0, do: "text-gray-900", else: "text-amber-500"}"}>
                          <%= :erlang.float_to_binary(@phone_multiplier / 1, decimals: 1) %>x
                        </span>
                        <span class="text-xs text-gray-600 block">/ 2x</span>
                      </div>
                    </div>
                  </div>
                  <!-- ROGUE Holdings Multiplier -->
                  <div class="flex items-center justify-between px-4 py-3">
                    <div class="flex items-center gap-2 shrink-0">
                      <div class="w-7 h-7 rounded-lg bg-gray-100 flex items-center justify-center overflow-hidden">
                        <img src="https://ik.imagekit.io/blockster/rogue-white-in-indigo-logo.png" alt="ROGUE" class="w-4 h-4" />
                      </div>
                      <span class="text-sm font-haas_medium_65 text-gray-600">ROGUE</span>
                    </div>
                    <div class="flex items-center gap-2">
                      <%= if @rogue_multiplier <= 1 do %>
                        <button phx-click="switch_tab" phx-value-tab="rogue" class="text-xs bg-gray-900 text-white w-16 py-1 rounded-lg hover:bg-gray-800 cursor-pointer font-haas_medium_65 text-center">Get</button>
                      <% end %>
                      <div class="text-right w-14">
                        <span class={"text-sm font-haas_bold_75 #{if @rogue_multiplier > 1, do: "text-gray-900", else: "text-gray-600"}"}>
                          <%= :erlang.float_to_binary(@rogue_multiplier / 1, decimals: 1) %>x
                        </span>
                        <span class="text-xs text-gray-600 block">/ 5x</span>
                      </div>
                    </div>
                  </div>
                  <!-- External Wallet Multiplier -->
                  <div class="flex items-center justify-between px-4 py-3">
                    <div class="flex items-center gap-2 shrink-0">
                      <div class="w-7 h-7 rounded-lg bg-gray-100 flex items-center justify-center">
                        <svg class="w-3.5 h-3.5 text-gray-400" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 15v2m-6 4h12a2 2 0 002-2v-6a2 2 0 00-2-2H6a2 2 0 00-2 2v6a2 2 0 002 2zm10-10V7a4 4 0 00-8 0v4h8z"></path>
                        </svg>
                      </div>
                      <span class="text-sm font-haas_medium_65 text-gray-600">Wallet</span>
                    </div>
                    <div class="flex items-center gap-2">
                      <%= if @wallet_multiplier <= 1 do %>
                        <button phx-click="switch_tab" phx-value-tab="wallet" class="text-xs bg-gray-900 text-white w-16 py-1 rounded-lg hover:bg-gray-800 cursor-pointer font-haas_medium_65 text-center">Connect</button>
                      <% end %>
                      <div class="text-right w-14">
                        <span class={"text-sm font-haas_bold_75 #{if @wallet_multiplier > 1, do: "text-gray-900", else: "text-gray-600"}"}>
                          <%= :erlang.float_to_binary(@wallet_multiplier / 1, decimals: 1) %>x
                        </span>
                        <span class="text-xs text-gray-600 block">/ 3.6x</span>
                      </div>
                    </div>
                  </div>
                  <!-- Overall Total -->
                  <div class="flex items-center justify-between px-4 py-3 bg-gray-50">
                    <div class="flex items-center gap-2 shrink-0">
                      <div class="w-7 h-7 rounded-lg bg-gray-100 flex items-center justify-center">
                        <svg xmlns="http://www.w3.org/2000/svg" class="w-3.5 h-3.5 text-gray-900" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 10V3L4 14h7v7l9-11h-7z" />
                        </svg>
                      </div>
                      <span class="text-sm font-haas_bold_75 text-gray-900">Total</span>
                    </div>
                    <div class="text-right w-14">
                      <span class="text-sm font-haas_bold_75 text-gray-900">
                        <%= :erlang.float_to_binary(@overall_multiplier / 1, decimals: 1) %>x
                      </span>
                      <span class="text-xs text-gray-600 block">/ 360x</span>
                    </div>
                  </div>
                </div>
                <!-- Footer info -->
                <div class="px-4 py-2 bg-gray-50 text-xs text-gray-600 border-t border-gray-200">
                  Multipliers are combined: X × Phone × ROGUE × Wallet
                </div>
              </div>
            <% end %>
          </div>

          <!-- ROGUE Balance Card -->
          <div class="bg-wallet-card border border-gray-200 rounded-2xl p-5">
            <div class="flex items-center justify-between mb-3">
              <h3 class="text-[10px] font-haas_medium_65 text-gray-500 uppercase tracking-widest">ROGUE Balance</h3>
              <img src="https://ik.imagekit.io/blockster/rogue-white-in-indigo-logo.png" alt="ROGUE" class="h-5 w-5 rounded-full" />
            </div>
            <p class="text-2xl sm:text-3xl font-haas_bold_75 text-gray-900 tracking-tight">
              <%= Number.Currency.number_to_currency(Map.get(@token_balances, "ROGUE", 0), unit: "", precision: 2) %>
            </p>
            <p class="text-[10px] text-gray-600 mt-1">
              <a href="https://app.uniswap.org/explore/pools/arbitrum/0x9876d52d698ffad55fef13f4d631c0300cf2dc8ef90c8dd70405dc06fa10b2ec" target="_blank" class="text-gray-900/70 hover:text-gray-900 transition-colors cursor-pointer font-haas_medium_65">Buy ROGUE</a>
            </p>
          </div>
        </div>
        <div class="w-full bg-wallet-card border border-gray-200 rounded-2xl md:p-6 p-3 mt-4">
          <!-- Desktop Tabs -->
          <div class="p-1.5 bg-gray-50 rounded-2xl max-md:hidden">
            <div class="flex w-full gap-1">
              <button
                phx-click="switch_tab"
                phx-value-tab="activity"
                class={"flex-1 px-3 py-2.5 font-haas_medium_65 text-sm leading-[1.4] cursor-pointer rounded-xl transition-all relative z-10 #{if @active_tab == "activity", do: "bg-gray-900 text-white", else: "text-gray-500 hover:text-gray-700"}"}
              >
                Activity
              </button>
              <button
                phx-click="switch_tab"
                phx-value-tab="following"
