                    <div class="text-right">
                      <p class="text-[10px] uppercase tracking-widest text-gray-500">Current Multiplier</p>
                      <p class="text-xl font-haas_bold_75 text-gray-900 mt-1">
                        <%= :erlang.float_to_binary(@rogue_multiplier / 1, decimals: 1) %>x
                        <span class="text-xs text-gray-500 font-normal">/ 5.0x</span>
