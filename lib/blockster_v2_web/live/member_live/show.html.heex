                          <path d="M14.4667 4.00498C13.432 3.52873 12.3395 3.19086 11.2174 3C11.0639 3.27595 10.9249 3.55987 10.8012 3.85056C9.60588 3.66949 8.39035 3.66949 7.19506 3.85056C7.07125 3.5599 6.93231 3.27598 6.77882 3C5.65597 3.19247 4.56283 3.53114 3.527 4.00747C1.47061 7.066 0.913155 10.0486 1.19188 12.9888C2.39616 13.8833 3.74408 14.5635 5.17707 15C5.49973 14.5637 5.78525 14.1009 6.03059 13.6164C5.5646 13.4415 5.11484 13.2256 4.68651 12.9714C4.79924 12.8892 4.90949 12.8045 5.01603 12.7223C6.26237 13.3115 7.62269 13.617 8.99997 13.617C10.3773 13.617 11.7376 13.3115 12.9839 12.7223C13.0917 12.8107 13.2019 12.8954 13.3134 12.9714C12.8843 13.226 12.4337 13.4423 11.9669 13.6177C12.2119 14.1019 12.4974 14.5644 12.8204 15C14.2546 14.5653 15.6036 13.8853 16.8081 12.99C17.1351 9.58032 16.2494 6.62516 14.4667 4.00498ZM6.38365 11.1806C5.60693 11.1806 4.96523 10.472 4.96523 9.60025C4.96523 8.72852 5.58463 8.0137 6.38117 8.0137C7.17771 8.0137 7.81445 8.72852 7.80082 9.60025C7.7872 10.472 7.17524 11.1806 6.38365 11.1806ZM11.6163 11.1806C10.8383 11.1806 10.1991 10.472 10.1991 9.60025C10.1991 8.72852 10.8185 8.0137 11.6163 8.0137C12.4141 8.0137 13.0459 8.72852 13.0322 9.60025C13.0186 10.472 12.4079 11.1806 11.6163 11.1806Z" fill="#141414">
                          </path>
                        </svg>
                      </span>
                      Discord
                      <span class="close hidden">
                        <svg fill="none" height="20" viewbox="0 0 20 20" width="20" xmlns="http://www.w3.org/2000/svg">
                          <path d="M13.3346 13.3333L10.0013 9.99997M10.0013 9.99997L6.66797 6.66663M10.0013 9.99997L13.3347 6.66663M10.0013 9.99997L6.66797 13.3333" stroke="white" stroke-linecap="round" stroke-linejoin="round" stroke-width="2">
                          </path>
                        </svg>
                      </span>
                    </div>
                  </label>
                  <label class="block social-pill">
                    <input hidden="" name="topics" type="checkbox" value="Lifestyle"/>
                    <div class="pill py-2.5 h-10 block flex gap-2 px-4 justify-center items-center rounded-[30px] bg-[#EEF0F4] text-sm font-haas_medium_55 leading-[1.4]">
                      <span class="icon">
                        <svg fill="none" height="18" viewbox="0 0 18 18" width="18" xmlns="http://www.w3.org/2000/svg">
                          <path d="M15.5365 7.66007C14.2018 7.66007 12.966 7.23597 11.9569 6.51523V11.7545C11.9569 14.3754 9.83117 16.5 7.20901 16.5C6.23064 16.5 5.32128 16.2043 4.56588 15.6974C3.29662 14.8455 2.46094 13.3972 2.46094 11.7544C2.46094 9.13371 4.5867 7.00907 7.20908 7.00912C7.42701 7.00902 7.64467 7.02382 7.86056 7.05331V7.63502L7.86044 9.678C7.65264 9.61209 7.43112 9.57632 7.20129 9.57632C6.00172 9.57632 5.02946 10.5483 5.02946 11.747C5.02946 12.5946 5.51545 13.3286 6.22415 13.6861C6.518 13.8342 6.84987 13.9177 7.20131 13.9177C8.39841 13.9177 9.36901 12.9497 9.37314 11.7544V1.5H11.9569V1.83022C11.966 1.92894 11.9791 2.02729 11.9962 2.12499C12.1755 3.14728 12.7871 4.02071 13.636 4.55014C14.206 4.90577 14.8647 5.09378 15.5365 5.09276L15.5365 7.66007Z" fill="#141414">
                          </path>
                        </svg>
                      </span>
                      TikTok
                      <span class="close hidden">
                        <svg fill="none" height="20" viewbox="0 0 20 20" width="20" xmlns="http://www.w3.org/2000/svg">
                          <path d="M13.3346 13.3333L10.0013 9.99997M10.0013 9.99997L6.66797 6.66663M10.0013 9.99997L13.3347 6.66663M10.0013 9.99997L6.66797 13.3333" stroke="white" stroke-linecap="round" stroke-linejoin="round" stroke-width="2">
                          </path>
                        </svg>
                      </span>
                    </div>
                  </label>
                </div>
              </div>
            </div>
          </div>
          <div class={"tabs-main tab-wrapper #{if @active_tab != "wallet", do: "hidden", else: ""}"} id="wallet" phx-hook="ConnectWalletHook">
            <div class="inner md:p-6 p-2" id="wallet-balances-fetcher" phx-hook="BalanceFetcherHook">
              <div class="flex justify-between gap-2 flex-wrap mb-6">
                <p class="!text-[16px] font-haas_medium_65 text-[#343434] leading-[24px]">
                  Connected Wallet
                </p>
                <%= if @connected_wallet do %>
                  <p class="!text-sm font-haas_medium_55 text-green-600 leading-[24px]">
                    <b class="font-haas_medium_65">‚úì Connected</b>
                  </p>
                <% else %>
                  <p class="!text-sm font-haas_medium_55 text-gray-500 leading-[24px]">
                    No wallet connected
                  </p>
                <% end %>
              </div>

              <%= if @connected_wallet do %>
                <!-- Connected Wallet Display -->
                <div class="bg-white border border-[#DDE1E7] rounded-[12px] p-4 mb-4">
                  <div class="flex items-center justify-between">
                    <div class="flex items-center gap-3">
                      <div class="w-10 h-10 rounded-full bg-gradient-to-br from-purple-400 to-blue-500 flex items-center justify-center">
                        <svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 15v2m-6 4h12a2 2 0 002-2v-6a2 2 0 00-2-2H6a2 2 0 00-2 2v6a2 2 0 002 2zm10-10V7a4 4 0 00-8 0v4h8z"></path>
                        </svg>
                      </div>
                      <div>
                        <p class="text-sm font-haas_medium_65 text-[#343434]"><%= String.capitalize(@connected_wallet.provider) %></p>
                        <p class="text-xs font-haas_roman_55 text-gray-500 font-mono">
                          <%= String.slice(@connected_wallet.wallet_address, 0..5) %><span>...</span><%= String.slice(@connected_wallet.wallet_address, -4..-1) %>
                        </p>
                      </div>
                    </div>
                    <button
                      phx-click="disconnect_wallet"
                      class="px-4 py-2 bg-red-50 hover:bg-red-100 text-red-600 rounded-full text-sm font-haas_medium_65 cursor-pointer transition-colors"
                    >
                      Disconnect
                    </button>
                  </div>

                  <div class="mt-4 pt-4 border-t border-gray-100">
                    <p class="text-xs text-gray-500 mb-2">Full Address</p>
                    <div class="flex items-center gap-2">
                      <a
                        href={"https://roguescan.io/address/#{@connected_wallet.wallet_address}"}
                        target="_blank"
                        class="flex-1 text-xs font-mono bg-gray-50 px-3 py-2 rounded text-blue-500 hover:underline cursor-pointer"
                      >
                        <%= @connected_wallet.wallet_address %>
                      </a>
                      <button
                        phx-hook="CopyToClipboard"
                        data-copy-text={@connected_wallet.wallet_address}
                        id={"copy-address-#{@connected_wallet.id}"}
                        class="p-2 hover:bg-gray-100 rounded cursor-pointer"
                        title="Copy address"
                      >
                        <svg class="w-4 h-4" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 16H6a2 2 0 01-2-2V6a2 2 0 012-2h8a2 2 0 012 2v2m-6 12h8a2 2 0 002-2v-8a2 2 0 00-2-2h-8a2 2 0 00-2 2v8a2 2 0 002 2z"></path>
                        </svg>
                      </button>
                    </div>
                  </div>
                </div>


                <!-- Token Balances with Multipliers -->
                <%= if @wallet_balances && map_size(@wallet_balances) > 0 do %>
                  <%
                    # Filter out zero-balance stablecoins and ARB
                    filtered_balances = @wallet_balances
                    |> Enum.reject(fn {symbol, data} ->
                      symbol in ["USDT", "USDC", "ARB"] && data.total == 0
                    end)
                    |> Enum.into(%{})

                    # Calculate multiplier per token (simplified logic based on typical amounts)
                    token_multipliers = %{
                      "ROGUE" => fn total ->
                        cond do
                          total >= 100_000 -> 4.0
                          total >= 50_000 -> 2.0
                          total >= 10_000 -> 1.0
                          total >= 1_000 -> 0.5
                          true -> 0.1
                        end
                      end,
                      "ETH" => fn total ->
                        cond do
                          total >= 10 -> 1.5
                          total >= 5 -> 1.0
                          total >= 1 -> 0.5
                          true -> 0.2
                        end
                      end,
                      "USDT" => fn total -> if total >= 1000, do: 0.3, else: 0.1 end,
                      "USDC" => fn total -> if total >= 1000, do: 0.3, else: 0.1 end,
                      "ARB" => fn total -> if total >= 100, do: 0.2, else: 0.05 end
                    }
                  %>

                  <%
                    # Calculate total of individual token boosts
                    base_connection_boost = 0.10
                    individual_boosts_sum = filtered_balances
                    |> Enum.map(fn {symbol, data} ->
                      multiplier_fn = Map.get(token_multipliers, symbol, fn _ -> 0.1 end)
                      multiplier_fn.(data.total)
                    end)
                    |> Enum.sum()

                    total_boost = base_connection_boost + individual_boosts_sum
                  %>

                  <!-- ROGUE Multiplier Calculation Guide -->
                  <div class="bg-white rounded-xl border border-gray-200 shadow-sm overflow-hidden mb-4">
                    <!-- Header -->
                    <div class="bg-gradient-to-r from-indigo-500 to-purple-500 px-5 py-3">
                      <div class="flex items-center gap-2">
                        <svg class="w-5 h-5 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 10V3L4 14h7v7l9-11h-7z"></path>
                        </svg>
                        <h4 class="text-sm font-haas_bold_75 text-white">Hold ROGUE to Boost your BUX Earnings</h4>
                      </div>
                      <p class="text-xs text-indigo-100 mt-1">Earn 0.4x more BUX rewards for every 100k ROGUE held in your wallets (4x max boost)</p>
                    </div>

                    <!-- Tiers Grid -->
                    <div class="p-5">
                      <div class="grid grid-cols-2 gap-3">
                        <!-- Tier 1 -->
                        <div class="bg-gray-50 rounded-lg p-3 border border-gray-100">
                          <div class="text-xs text-gray-500 mb-1">100k - 199k ROGUE</div>
                          <div class="text-lg font-haas_bold_75 text-gray-900">+0.4x</div>
                        </div>

                        <!-- Tier 2 -->
                        <div class="bg-gray-50 rounded-lg p-3 border border-gray-100">
                          <div class="text-xs text-gray-500 mb-1">200k - 299k ROGUE</div>
                          <div class="text-lg font-haas_bold_75 text-gray-900">+0.8x</div>
                        </div>

                        <!-- Tier 3 -->
                        <div class="bg-gray-50 rounded-lg p-3 border border-gray-100">
                          <div class="text-xs text-gray-500 mb-1">300k - 399k ROGUE</div>
                          <div class="text-lg font-haas_bold_75 text-gray-900">+1.2x</div>
                        </div>

                        <!-- Tier 4 -->
                        <div class="bg-gray-50 rounded-lg p-3 border border-gray-100">
                          <div class="text-xs text-gray-500 mb-1">400k - 499k ROGUE</div>
                          <div class="text-lg font-haas_bold_75 text-gray-900">+1.6x</div>
                        </div>

                        <!-- Tier 5 -->
                        <div class="bg-gradient-to-br from-green-50 to-emerald-50 rounded-lg p-3 border border-green-200">
                          <div class="text-xs text-green-700 mb-1">500k - 599k ROGUE</div>
                          <div class="text-lg font-haas_bold_75 text-green-600">+2x</div>
                        </div>

                        <!-- Tier 6 -->
                        <div class="bg-gradient-to-br from-green-50 to-emerald-50 rounded-lg p-3 border border-green-200">
                          <div class="text-xs text-green-700 mb-1">600k - 699k ROGUE</div>
                          <div class="text-lg font-haas_bold_75 text-green-600">+2.4x</div>
                        </div>

                        <!-- Tier 7 -->
                        <div class="bg-gradient-to-br from-green-50 to-emerald-50 rounded-lg p-3 border border-green-200">
                          <div class="text-xs text-green-700 mb-1">700k - 799k ROGUE</div>
                          <div class="text-lg font-haas_bold_75 text-green-600">+2.8x</div>
                        </div>

                        <!-- Tier 8 -->
                        <div class="bg-gradient-to-br from-green-50 to-emerald-50 rounded-lg p-3 border border-green-200">
                          <div class="text-xs text-green-700 mb-1">800k - 899k ROGUE</div>
                          <div class="text-lg font-haas_bold_75 text-green-600">+3.2x</div>
                        </div>

                        <!-- Tier 9 -->
                        <div class="bg-gradient-to-br from-green-50 to-emerald-50 rounded-lg p-3 border border-green-200">
                          <div class="text-xs text-green-700 mb-1">900k - 999k ROGUE</div>
                          <div class="text-lg font-haas_bold_75 text-green-600">+3.6x</div>
                        </div>

                        <!-- Tier 10 - Max -->
                        <div class="bg-gradient-to-br from-green-100 to-emerald-100 rounded-lg p-3 border-2 border-green-300 relative overflow-hidden">
                          <div class="absolute top-0 right-0 bg-green-500 text-white text-[10px] font-haas_bold_75 px-2 py-0.5 rounded-bl-lg">MAX</div>
                          <div class="text-xs text-green-800 mb-1">1M+ ROGUE</div>
                          <div class="text-lg font-haas_bold_75 text-green-600">+4x</div>
                        </div>
                      </div>

                      <!-- Formula -->
                      <div class="mt-4 bg-indigo-50 rounded-lg p-3 border border-indigo-100">
                        <div class="flex items-start gap-2">
                          <svg class="w-4 h-4 text-indigo-600 mt-0.5 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 16h-1v-4h-1m1-4h.01M21 12a9 9 0 11-18 0 9 9 0 0118 0z"></path>
                          </svg>
                          <div class="text-xs text-indigo-900">
                            <span class="font-haas_bold_75">Formula:</span> Rogue Chain + (Arbitrum One √ó 0.5) = Weighted Total
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>

                  <div class="bg-white rounded-xl border border-gray-200 overflow-hidden mb-4">
                    <div class="bg-gray-50 px-4 py-3 border-b border-gray-200 flex items-center justify-between">
                      <h3 class="text-sm font-haas_medium_65 text-gray-900"><%= String.capitalize(@connected_wallet.provider) %> Wallet Balances</h3>
                      <button
                        phx-click="refresh_balances"
                        class="flex items-center gap-1.5 px-2.5 py-1 text-xs text-gray-600 hover:text-gray-900 hover:bg-gray-100 rounded-lg cursor-pointer transition-colors"
                        title="Refresh balances"
                      >
                        <svg class="w-3.5 h-3.5" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 4v5h.582m15.356 2A8.001 8.001 0 004.582 9m0 0H9m11 11v-5h-.581m0 0a8.003 8.003 0 01-15.357-2m15.357 2H15"></path>
                        </svg>
                      </button>
                    </div>

                    <!-- Column Headers -->
                    <div class="px-4 py-2 bg-gray-50 border-b border-gray-100 flex items-center justify-between text-xs font-haas_medium_65 text-gray-500">
                      <div class="flex items-center gap-3 flex-1">
                        <div class="w-32">Token</div>
                        <div>Balance</div>
                      </div>
                      <div class="flex items-center gap-2">
                        <span>BUX Multiplier</span>
                      </div>
                    </div>

                    <div class="divide-y divide-gray-100">
                      <!-- Base + Connection row (always 1.1x when wallet connected) -->
                      <div class="px-4 py-3 flex items-center justify-between bg-blue-50">
                        <div class="flex items-center gap-3 flex-1">
                          <div class="text-sm font-haas_medium_65 text-gray-900 w-32">-</div>
                          <div class="text-sm text-gray-600">
                            Base + Wallet Connected
                          </div>
                        </div>
                        <div class="flex items-center gap-2">
                          <div class="text-xs text-green-600 font-haas_medium_65">
                            1.1x
                          </div>
                          <svg class="w-4 h-4 text-green-500" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 13l4 4L19 7"></path>
                          </svg>
                        </div>
                      </div>

                      <%= for {symbol, data} <- filtered_balances do %>
                        <%= if symbol == "ROGUE" do %>
                          <%
                            # Get USD price for ROGUE
                            price_data = case BlocksterV2.PriceTracker.get_price(symbol) do
                              {:ok, price} -> price
                              _ -> nil
                            end

                            # Display each chain separately for ROGUE
                            chains_to_display = data.chains
                            |> Enum.filter(fn chain -> chain.chain_id in [560013, 42161] end)
                            |> Enum.sort_by(fn chain -> chain.chain_id end, :desc)  # Rogue Chain first (560013), then Arbitrum (42161)
                          %>
                          <%= for chain <- chains_to_display do %>
                            <%
                              multiplier_fn = Map.get(token_multipliers, symbol, fn _ -> 0.1 end)
                              boost = multiplier_fn.(chain.balance)
                              usd_value = if price_data, do: chain.balance * price_data.usd_price, else: nil
                              chain_name = case chain.chain_id do
                                560013 -> "Rogue Chain"
                                42161 -> "Arbitrum One"
                                _ -> chain.chain_name
                              end
                            %>
                            <div class="px-4 py-3 flex items-center justify-between hover:bg-gray-50 transition-colors">
                              <div class="flex items-center gap-3 flex-1">
                                <div class="w-32">
                                  <div class="flex items-center gap-2">
                                    <img src="https://ik.imagekit.io/blockster/rogue-white-in-indigo-logo.png" alt="ROGUE" class="w-5 h-5 rounded-full" />
                                    <span class="text-sm font-haas_medium_65 text-gray-900"><%= symbol %></span>
                                  </div>
                                  <div class="text-xs text-gray-500 ml-7 whitespace-nowrap"><%= chain_name %></div>
                                </div>
                                <div>
                                  <div class="text-sm text-gray-900 font-haas_medium_65">
                                    <%= Number.Delimit.number_to_delimited(chain.balance, precision: 2) %>
                                  </div>
                                  <%= if usd_value do %>
                                    <div class="text-xs text-gray-500">
                                      $<%= Number.Delimit.number_to_delimited(usd_value, precision: 2) %>
                                    </div>
                                  <% end %>
                                </div>
                              </div>
                              <div class="flex items-center gap-2">
                                <div class="text-xs text-green-600 font-haas_medium_65">
                                  +<%= if boost == trunc(boost), do: trunc(boost), else: :erlang.float_to_binary(boost, decimals: 1) %>x
                                </div>
                                <svg class="w-4 h-4 text-green-500" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 10V3L4 14h7v7l9-11h-7z"></path>
                                </svg>
                              </div>
                            </div>
                          <% end %>
                        <% else %>
                          <%
                            # Get USD price for this token
                            price_data = case BlocksterV2.PriceTracker.get_price(symbol) do
                              {:ok, price} -> price
                              _ -> nil
                            end
                            multiplier_fn = Map.get(token_multipliers, symbol, fn _ -> 0.1 end)
                            boost = multiplier_fn.(data.total)
                            usd_value = if price_data, do: data.total * price_data.usd_price, else: nil
                          %>
                          <div class="px-4 py-3 flex items-center justify-between hover:bg-gray-50 transition-colors">
                            <div class="flex items-center gap-3 flex-1">
                              <div class="flex items-center gap-2 w-32">
                                <%= if symbol == "ETH" do %>
                                  <img src="https://ik.imagekit.io/blockster/ethereum.png" alt="ETH" class="w-5 h-5" />
                                <% end %>
                                <span class="text-sm font-haas_medium_65 text-gray-900"><%= symbol %></span>
                              </div>
                              <div>
                                <div class="text-sm text-gray-900 font-haas_medium_65">
                                  <%= :erlang.float_to_binary(data.total / 1, decimals: if(data.total < 10, do: 4, else: 2)) %>
                                </div>
                                <%= if usd_value do %>
                                  <div class="text-xs text-gray-500">
                                    $<%= Number.Delimit.number_to_delimited(usd_value, precision: 2) %>
                                  </div>
                                <% end %>
                              </div>
                            </div>
                            <div class="flex items-center gap-2">
                              <div class="text-xs text-green-600 font-haas_medium_65">
                                +<%= if boost == trunc(boost), do: trunc(boost), else: :erlang.float_to_binary(boost, decimals: 1) %>x
                              </div>
                              <svg class="w-4 h-4 text-green-500" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 10V3L4 14h7v7l9-11h-7z"></path>
                              </svg>
                            </div>
                          </div>
                        <% end %>
                      <% end %>
                    </div>

                    <!-- Total Multiplier -->
                    <div class="bg-gradient-to-r from-green-50 to-emerald-50 px-4 py-3 border-t-2 border-green-300">
                      <div class="flex items-center justify-between">
                        <span class="text-sm font-haas_bold_75 text-green-900">Total BUX Multiplier</span>
                        <%
                          # Full wallet multiplier includes base 1.0
                          full_wallet_multiplier = 1.0 + total_boost
                        %>
                        <span class="text-lg font-haas_bold_75 text-green-600"><%= if full_wallet_multiplier == trunc(full_wallet_multiplier), do: trunc(full_wallet_multiplier), else: :erlang.float_to_binary(full_wallet_multiplier, decimals: 1) %>x</span>
                      </div>
                      <p class="text-xs text-green-700 mt-1">Updates daily at 3:00 AM UTC</p>
                    </div>
                  </div>
                <% else %>
                  <%= if @connected_wallet do %>
                    <div class="bg-gray-50 rounded-xl border border-gray-200 p-6 mb-4 text-center">
                      <p class="text-sm text-gray-600">
                        <button
                          phx-click="refresh_balances"
                          class="font-haas_medium_65 text-blue-600 hover:text-blue-700 underline cursor-pointer"
                        >
                          Click here
                        </button>
                        to fetch your token balances
                      </p>
                    </div>
                  <% end %>
                <% end %>

                <!-- Wallet Actions -->
                <div class="grid md:grid-cols-2 gap-4">
                  <!-- Send to Blockster (Hardware ‚Üí Smart Wallet) -->
