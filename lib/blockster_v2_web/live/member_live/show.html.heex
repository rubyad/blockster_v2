                <!-- Tiers - Vertical Progress Tracker -->
                <div class="p-5">
                  <p class="text-[10px] uppercase tracking-widest text-gray-500 font-haas_medium_65 mb-4">ROGUE Holding Tiers</p>
                  <div class="space-y-0">
                    <%
                      rogue_balance = @rogue_multiplier_data.balance
                      tiers = [
                        {0, 99_999, 0.0, "0 - 99,999 ROGUE"},
                        {100_000, 199_999, 0.4, "100k - 199k ROGUE"},
                        {200_000, 299_999, 0.8, "200k - 299k ROGUE"},
                        {300_000, 399_999, 1.2, "300k - 399k ROGUE"},
                        {400_000, 499_999, 1.6, "400k - 499k ROGUE"},
                        {500_000, 599_999, 2.0, "500k - 599k ROGUE"},
                        {600_000, 699_999, 2.4, "600k - 699k ROGUE"},
                        {700_000, 799_999, 2.8, "700k - 799k ROGUE"},
                        {800_000, 899_999, 3.2, "800k - 899k ROGUE"},
                        {900_000, 999_999, 3.6, "900k - 999k ROGUE"},
                        {1_000_000, :infinity, 4.0, "1M+ ROGUE"}
                      ]
                    %>
                    <%= for {min_val, max_val, boost, label} <- tiers do %>
                      <%
                        is_current = if max_val == :infinity do
                          rogue_balance >= min_val
                        else
                          rogue_balance >= min_val && rogue_balance <= max_val
                        end
                        is_achieved = rogue_balance >= min_val
                        is_max = max_val == :infinity
                      %>
                      <div class="flex items-center gap-3 group">
                        <!-- Progress line + dot -->
                        <div class="flex flex-col items-center">
                          <div class={[
                            "w-3 h-3 rounded-full flex-shrink-0 border-2",
                            if(is_current, do: "bg-gray-900 border-gray-900 shadow-sm", else: if(is_achieved, do: "bg-gray-400 border-gray-400", else: "bg-transparent border-gray-600"))
                          ]}>
                          </div>
                          <%= unless is_max do %>
                            <div class={if is_achieved, do: "w-0.5 h-6 bg-gray-200", else: "w-0.5 h-6 bg-gray-100"}></div>
                          <% end %>
                        </div>
                        <!-- Tier info -->
                        <div class={"flex items-center justify-between flex-1 py-1 #{if is_current, do: "", else: "opacity-60 group-hover:opacity-80 transition-opacity"}"}>
                          <span class={"text-xs #{if is_current, do: "text-gray-900 font-haas_medium_65", else: if(is_achieved, do: "text-gray-600", else: "text-gray-500")}"}}>
                            <%= label %>
                          </span>
                          <div class="flex items-center gap-2">
                            <span class={"text-sm font-haas_bold_75 #{if is_current, do: "text-gray-900", else: if(is_achieved, do: "text-gray-600", else: "text-gray-600")}"}>
                              +<%= if boost == trunc(boost), do: trunc(boost), else: :erlang.float_to_binary(boost, decimals: 1) %>x
                            </span>
                            <%= if is_max && is_current do %>
                              <span class="bg-gray-900 text-white text-[10px] font-haas_bold_75 px-2 py-0.5 rounded-lg">
                                MAX
                              </span>
                            <% end %>
                            <%= if is_achieved && !is_current do %>
                              <svg class="w-3.5 h-3.5 text-gray-900/60" fill="currentColor" viewBox="0 0 20 20"><path fill-rule="evenodd" d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z" clip-rule="evenodd"></path></svg>
                            <% end %>
                          </div>
                        </div>
                      </div>
                    <% end %>
                  </div>

                  <!-- Info Note -->
                  <div class="mt-4 bg-gray-50 rounded-xl p-3 border border-gray-200">
                    <div class="flex items-start gap-2">
                      <svg class="w-4 h-4 text-gray-500 mt-0.5 flex-shrink-0" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                        <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 16h-1v-4h-1m1-4h.01M21 12a9 9 0 11-18 0 9 9 0 0118 0z"></path>
                      </svg>
                      <div class="text-xs text-gray-500">
                        <span class="font-haas_bold_75 text-gray-400">Note:</span> Only ROGUE in your Blockster smart wallet counts toward this multiplier. External wallet ROGUE does not count.
                        <%= if @rogue_multiplier_data.balance > 1_000_000 do %>
                          <span class="block mt-1">Only the first 1M ROGUE counts toward the multiplier (you have <%= Number.Delimit.number_to_delimited(@rogue_multiplier_data.balance - 1_000_000, precision: 0) %> ROGUE above the cap).</span>
                        <% end %>
                      </div>
                    </div>
                  </div>
                </div>
              </div>

              <!-- Play BUX Booster - Full Width Bottom -->
              <.link navigate={~p"/play"} class="block w-full bg-gray-900 rounded-2xl p-4 hover:bg-gray-800 transition-all cursor-pointer">
                <div class="flex items-center justify-center gap-3">
                  <img src="https://ik.imagekit.io/blockster/blockster-icon.png" alt="BUX Booster" class="w-10 h-10 rounded-full" />
                  <div class="text-left">
                    <p class="font-haas_bold_75 text-white text-lg">Play BUX Booster</p>
                    <p class="text-sm text-white/60">Win ROGUE with coin flips</p>
                  </div>
                </div>
              </.link>
            </div>
          </div>

          <div class={"tab-wrapper #{if @active_tab != "wallet", do: "hidden", else: ""}"} id="wallet" phx-hook="ConnectWalletHook">
            <div class="inner md:p-6 p-2 bg-gray-50 rounded-xl mt-4" id="wallet-balances-fetcher" phx-hook="BalanceFetcherHook">
              <div class="flex justify-between gap-2 flex-wrap mb-4">
                <p class="text-base font-haas_medium_65 text-gray-900 leading-[24px]">
                  Connected Wallet
                </p>
                <%= if @connected_wallet do %>
                  <p class="text-sm font-haas_medium_65 text-gray-900 leading-[24px]">
                    ✓ Connected
                  </p>
                <% else %>
                  <p class="text-sm font-haas_medium_65 text-gray-500 leading-[24px]">
                    No wallet connected
                  </p>
                <% end %>
              </div>

              <%= if @connected_wallet do %>
                <!-- Connected Wallet Display -->
                <div class="bg-wallet-card border border-gray-200 rounded-2xl p-4 mb-4">
                  <div class="flex items-center justify-between">
                    <div class="flex items-center gap-3">
                      <div class="w-10 h-10 rounded-xl bg-gray-100 flex items-center justify-center">
                        <svg class="w-5 h-5 text-gray-900" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                          <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 15v2m-6 4h12a2 2 0 002-2v-6a2 2 0 00-2-2H6a2 2 0 00-2 2v6a2 2 0 002 2zm10-10V7a4 4 0 00-8 0v4h8z"></path>
                        </svg>
                      </div>
                      <div>
                        <p class="text-sm font-haas_medium_65 text-gray-900"><%= String.capitalize(@connected_wallet.provider) %></p>
                        <p class="text-xs font-haas_roman_55 text-gray-500 font-mono">
